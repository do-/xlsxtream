{
  "name": "xlsxtream",
  "version": "2.0.3",
  "description": "Reading .xlsx worksheets as object streams",
  "main": "index.js",
  "files": [
    "/lib",
    "/badges"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "text",
      "json-summary"
    ],
    "verbose": false,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/data",
      "/__tests__/lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/do-/xlsxtream.git"
  },
  "author": "Dmitry Ovsyanko",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "dependencies": {
    "node-stream-zip": "^1.15.0",
    "xml-toolkit": "^1.0.47"
  }
}
